<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <link rel="stylesheet" href="main.css">
</head>
<body>
    <%-include('nav.ejs') %>
    <section class="sec member">
        <div class="container text-center">
            <table>
                <tr>
                    <th class="col">id</th>
                    <th class="col">name</th>
                    <th class="col">team</th>
                    <th class="col">position</th>
                    <th class="col">emailAdress</th>
                    <th class="col">admissionDate</th>
                    <th class="col">birthday</th>
                </tr>
                <% for(i of member) {%>
                <tr>
                    <td><%= i.id %></td>
                    <td><%= i.name %></td>
                    <td><%= i.team %></td>
                    <td><%= i.position %></td>
                    <td><%= i.emailAddress %></td>
                    <td><%= i.admissionDate.toLocaleDateString()%></td>
                    <td><%= i.birthday.toLocaleDateString() %></td>
                </tr>
                <% } %>
            </table>
        </div>
    </section>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
</body>
</html>